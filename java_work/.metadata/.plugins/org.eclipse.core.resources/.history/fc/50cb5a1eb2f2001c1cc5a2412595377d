
public class FF {
	public static void main(String[] args) {

		
		boolean a = true;
		boolean aa = false;
		System.out.println(a);
		char b = 'A';
		int c = 123123;
		
		float kk =1.2f; // 마지막에 f쓰는이유는 float 성질임 아무의미없음
		double qq = 1.3d;// d도 마찬가지 이유.
		
		System.out.println(kk);System.out.println(qq);
				
		
		
	}
}
