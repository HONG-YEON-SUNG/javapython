package aa;


public class BB {

	
	//c언어에서 ex01.c -소스파일인데 >>>(컴파일하면) ex01.exe
	//자바에서는 AA.java >>>(컴파일하면) AA.classs 실행
	
	

	//main쓰고 컨트롤+스페이스 = main함수 밑에 줄처럼 자동완성 (이클립스에서 지원해주는 내장함수)
	
	public static void main(String[] args) {
		System.out.println(1+3);
		System.out.println(10*4);
		System.out.println(22-33);
		
	}
	
}
