package ex0624;

public class AA {

	public static void main(String[] args) {
		
		int a = 10;
		
		System.out.println("a="+a);
		
		
		
		//boolean = 기본 자료형 int는 기본 숫자형
		boolean b = true;
		
		long c = 123213125414L; // long 기본형은 L로 단위써야함
		
		
		System.out.println("b="+b);
		System.out.println("c="+c);
		
		
		
	}
}
